{
  "name": "profileprep",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "proxy": "cd auth-proxy && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run proxy\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:format": "npx prisma format",
    "db:migrate-cr": "npx prisma migrate dev --create-only",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate-pr": "npx prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.1.13",
    "@auth/prisma-adapter": "^2.7.4",
    "@prisma/client": "^6.4.1",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@tanstack/react-query": "^5.66.9",
    "@tanstack/react-query-devtools": "^5.66.9",
    "@tanstack/react-query-next-experimental": "^5.66.9",
    "@vercel/analytics": "^1.5.0",
    "ai": "^4.1.38",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "html-to-pdfmake": "^2.5.21",
    "lucide-react": "^0.475.0",
    "markdown-it": "^14.1.0",
    "motion": "^12.4.5",
    "next": "^15.2.0",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.4",
    "pdfmake": "^0.2.18",
    "react": "^19.0.0",
    "react-confetti": "^6.2.2",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-hot-toast": "^2.5.1",
    "react-pdftotext": "^1.3.4",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
