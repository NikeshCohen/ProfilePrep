{
  "name": "profileprep",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "proxy": "cd auth-proxy && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run proxy\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "db:format": "npx prisma format",
    "db:migrate-cr": "npx prisma migrate dev --create-only",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate-pr": "npx prisma migrate deploy",
    "postinstall": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "eslint:check": "eslint .",
    "eslint": "eslint . --fix && prettier --write ."
